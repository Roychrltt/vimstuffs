snippet quicksort
void	quicksort(std::vector<int>& v, int l, int r)
{
	if (l >= r) return;
	int ll = l, rr = r, k = v[l];
	while (l < r)
	{
		while (r > l && v[r] >= k) r--;
		v[l] = v[r];
		while (l < r && v[l] <= k) l++;
		v[r] = v[l];
	}
	v[l] = k;
	quicksort(v, ll, l - 1);
	quicksort(v, r + 1, rr);
}
endsnippet

snippet quickselect
int partition(std::vector<int>& v, int l, int r)
{
	int pivot = v[r], i = l;
	for (int j = l, j < r; j++)
		if (v[j] <= pivot) std::swap(v[j], v[i++]);

	std::swap(v[i], v[r]);
	return i;
}

int kthSmallest(std::vector<int>& v, int l, int r, int k)
{
	if (k <= 0 || k > r - l + 1) return -1;

	int idx = partition(v, l, r);
	if (idx - l == k - 1) return v[idx];
	else if (idx - 1 > k - 1) return kthSmallest(v, l, idx - 1, k);
	else return kthSmallest(v, idx + 1, r, k - idx + l - 1);
}
endsnippet

snippet bit
class BIT {
	public:
		std::vector<int> bit;
		int n;

		BIT(int n)
		{
			this->n = n + 1;
			bit.assign(n + 1, 0);
		}

		BIT(const std::vector<int>& v)
		{
			n = v.size() + 1;
			bit.resize(n);
			for (size_t i = 0; i < v.size(); i++)
				add(i + 1, v[i]);
		}

		int sum(int i)
		{
			if (i > n) i = n;
			int ans = 0;
			while (i > 0)
			{
				ans += bit[i];
				i -= i & -i;
			}
			return ans;
		}

		void add(int i, int d)
		{
			while (i < n)
			{
				bit[i] += d;
				i += i & -i;
			}
		}
};
endsnippet

